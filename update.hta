<script language="VBScript">
   Function XorDecrypt(hexStr, key)
    Dim i, result, keyLen, c, k
    result = ""
    keyLen = Len(key)
    For i = 1 To Len(hexStr) Step 2
        c = CInt("&H" & Mid(hexStr, i, 2))
        k = Asc(Mid(key, (((i \ 2)) Mod keyLen) + 1, 1))
        result = result & Chr(c Xor k)
    Next
    XorDecrypt = result
End Function

